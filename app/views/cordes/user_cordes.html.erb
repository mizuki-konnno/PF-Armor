<div class = "container" >
  <div class = "row my-5" >
    <div class = "col text-center" >
      <h1>投稿コーデ一覧</h1>
    </div>
  </div>
  <div class = "row">
    <div class = "col text-center">
      <%= link_to "戻る", :back, class: "btn btn-outline-secondary mt-5" %>
    </div>
  </div>
  <div class = "row">
    <% @cordes.each do |corde| %>
    <div class = "col-2 mt-5 slide-bottom show">
      <%= link_to corde_path(corde.id) do %>
      <%= attachment_image_tag(corde, :corde_image, size: "150x180", fallback: "no_image.jpg") %>
      <% end %>
    </div>
    <% end %>
  </div>
  <div class = "row my-5" >
    <div class = "col-3 mx-auto" >
      <%= paginate @cordes %>
    </div>
  </div>
</div>